{
  "id": null,
  "title": "RAG API Dashboard",
  "tags": ["rag", "api"],
  "style": "dark",
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "API Requests",
      "type": "stat",
      "targets": [
        {
          "expr": "rate(http_requests_total[5m])",
          "legendFormat": "Requests/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "displayMode": "list"
          }
        }
      }
    },
    {
      "id": 2,
      "title": "Response Time",
      "type": "graph",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
          "legendFormat": "95th percentile"
        }
      ]
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "refresh": "5s"
}